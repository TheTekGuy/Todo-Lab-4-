<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.example.usmanhussain.Todoapp">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="com.example.usmanhussain.Todoapp.TodoListActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN"/>

            <category android:name="android.intent.category.LAUNCHER"/>
        </intent-filter>
    </activity>
    <activity
        android:name="com.example.usmanhussain.Todoapp.TodoActivity"
        android:parentActivityName="com.example.usmanhussain.Todoapp.TodoListActivity">
    </activity>
    <activity
        android:name="com.example.usmanhussain.Todoapp.TodoPagerActivity"
        android:parentActivityName="com.example.usmanhussain.Todoapp.TodoListActivity">
    </activity>

    <activity
    android:name="com.example.usmanhussain.Todoapp.TodoSearchActivity">
</activity>

    <activity
        android:name="com.example.usmanhussain.Todoapp.TodoSearchPageActivity">
        android:parentActivityName="com.example.usmanhussain.Todoapp.TodoSearchActivity">

    </activity>

</application>

</manifest>

